<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css">
    <link rel="shortcut icon" type="image/x-icon" href="images/favicon.png"/>
    <link rel="stylesheet" type="text/css" href="/css/bootstrap.css">
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
    <body>
    <div class="container justify-content-center" style="margin-top: 6%">
        <% data.forEach(function(x) { %>

        <div class="row">
            <div class="col-md-12">
                <div class="row mb-5 mt-2">
                    <div class="col-md-12" style="text-align: center">
                        <p style="font-size: 32px;color:gray;font-weight: bold;">Uspešno ste završili test, rezultate istog možete videti u nastavku!</p>
                    </div>
                </div>
                <div class="row my-3">
                    <div class="col-md-2"></div>
                    <div class="col-md-8">
                        <div class="input-group input-group-lg">
                            <div class="input-group-prepend">
                                <span class="input-group-text" id="inputGroup-sizing-lg" style="width: 280px;font-size: 16px;font-weight: bold;">Ime:</span>
                            </div>
                            <input type="text" class="form-control" value="<%=x.ime%>" style="border-radius: 5px;color:darkslateblue;max-width: 480px;" readonly>
                        </div>
                    </div>
                    <div class="col-md-2"></div>
                </div>
                <div class="row my-3">
                    <div class="col-md-2"></div>
                    <div class="col-md-8">
                        <div class="input-group input-group-lg">
                            <div class="input-group-prepend">
                                <span class="input-group-text" id="inputGroup-sizing-lg" style="width: 280px; font-size: 16px;font-weight: bold;">Prezime:</span>
                            </div>
                            <input type="text" class="form-control" value="<%=x.prezime%>" style="border-radius: 5px;color:darkslateblue;max-width: 480px;" readonly>
                        </div>
                    </div>
                    <div class="col-md-2"></div>
                </div>
                <div class="row my-3">
                    <div class="col-md-2"></div>
                    <div class="col-md-8">
                        <div class="input-group input-group-lg">
                            <div class="input-group-prepend">
                                <span class="input-group-text" id="inputGroup-sizing-lg" style="width: 280px; font-size: 16px;font-weight: bold;">Email:</span>
                            </div>
                            <input type="text" class="form-control" value="<%=x.email%>" style="border-radius: 5px;color:darkslateblue;max-width: 480px;" readonly>
                        </div>
                    </div>
                    <div class="col-md-2"></div>
                </div>
                <% dataA.forEach(function(y) { %>
                <div class="row my-3">
                    <div class="col-md-2"></div>
                    <div class="col-md-4">
                        <div class="input-group input-group-lg">
                            <div class="input-group-prepend">
                                <span class="input-group-text" id="inputGroup-sizing-lg" style="width: 230px; font-size: 16px;font-weight: bold;">Broj tačnih odgovora:</span>
                            </div>
                            <input type="text" class="form-control" value="<%=x.correct%>" style="border-radius: 5px;font-weight:bold;color:green;" readonly>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="input-group input-group-lg">
                            <div class="input-group-prepend">
                                <span class="input-group-text" id="inputGroup-sizing-lg" style="width: 250px; font-size: 16px;font-weight: bold;">Procenat tačnih odgovora:</span>
                            </div>
                            <input type="text" class="form-control" value="<%=y.correctA%>" style="border-radius: 5px;font-weight: bold;color:green;" readonly>
                        </div>
                    </div>
                    <div class="col-md-2"></div>
                </div>
                <div class="row my-3">
                    <div class="col-md-2"></div>
                    <div class="col-md-4">
                        <div class="input-group input-group-lg">
                            <div class="input-group-prepend">
                                <span class="input-group-text" id="inputGroup-sizing-lg" style="width: 230px; font-size: 16px;font-weight: bold;">Broj netačnih odgovora:</span>
                            </div>
                            <input type="text" class="form-control" value="<%=x.wrong%>" style="border-radius: 5px;color:red;font-weight: bold;" readonly >
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="input-group input-group-lg">
                            <div class="input-group-prepend">
                                <span class="input-group-text" id="inputGroup-sizing-lg" style="width: 250px; font-size: 16px;font-weight: bold;">Procenat netačnih odgovora:</span>
                            </div>
                            <input type="text" class="form-control" value="<%=y.wrongA%>" style="border-radius: 5px;font-weight: bold;color:red;" readonly>
                        </div>
                    </div>
                    <div class="col-md-2"></div>
                </div>


                <div class="row justify-content-center mt-5">
                    <div class="col-md-2"></div>
                    <div class="col-md-8">
                        <div class="input-group input-group-lg">
                            <div class="input-group-prepend">
                                <span class="input-group-text" id="inputGroup-sizing-lg" style="width: 280px; font-size: 16px;font-weight: bold;">Status kandidata:</span>
                            </div>
                            <% if(y.stats === 'Položio!'){ %>
                                <input type="text" class="form-control" value="<%=y.stats%>" style="border-radius: 5px;font-weight:bold;color:green;border-color:green;max-width: 480px;" readonly>
                            <% } else{ %>
                                <input type="text" class="form-control" value="<%=y.stats%>" style="border-radius: 5px;font-weight:bold;color:red;border-color:red;max-width: 480px;" readonly>
                            <% } %>


                        </div>
                    </div>
                    <div class="col-md-2"></div>
                </div>
                <div class="row mt-5" style="text-align: center;">
                    <div class="col-md-2"></div>
                    <div class="col-md-8 justify-content-center">
                        <a href="/logout"><button type="button" class="btn btn-warning" style="width: 228px; text-align: center">Odjavi se</button></a>
                    </div>
                    <div class="col-md-2"></div>
                </div>
                <% }) %>
            </div>
        </div>

        <% }) %>
    </div>
    <script type="text/javascript" src="/jsj/jquery.js"></script>
    <script type="text/javascript" src="/js/bootstrap.js"></script>
    </body>
</html>
